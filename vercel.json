{
  "version": 2,
  "builds": [
    {
      "src": "passenger_wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "staticfiles/**/*",
      "use": "@vercel/static"
    }
  ],

  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "max-age=31536000, immutable" },
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/passenger_wsgi.py"
    }
  ]
}
