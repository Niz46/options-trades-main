{
  "version": 3.12,
  "builds": [
    {
      "src": "requirements.txt",
      "use": "@vercel/python"
    },
    {
      "src": "passenger_wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(static|media)/(.*)",
      "headers": { "cache-control": "max-age=31536000, immutable" },
      "dest": "passenger_wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "passenger_wsgi.py"
    }
  ]
}
